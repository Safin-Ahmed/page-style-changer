{
  "manifest_version": 3,
  "name": "PageStyle Changer",
  "version": "1.0",
  "description": "Changes Page Styles According To Your Preferences",
  "icon": {
    "128": "icon128.png",
    "48": "icon32.png",
    "16": "icon16.png"
  },
  "action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "PageFontStyle"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"]
}
